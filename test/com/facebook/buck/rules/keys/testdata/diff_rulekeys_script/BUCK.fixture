java_library(
    name = "java_lib_1",
    srcs = ["JavaLib1.java"],
)

java_library(
    name = "java_lib_2",
    # srcs is a glob because JavaLib3.java is added by a test.
    srcs = glob([
        "JavaLib2.java",
        "JavaLib3.java",
    ]),
    deps = [":java_lib_1"],
)

# This is the same as java_lib_2 except that it has no deps on any of the other rules.
java_library(
    name = "java_lib_all",
    # srcs is a glob because JavaLib3.java is added by a test.
    srcs = glob([
        "JavaLib3.java",
    ]),
)
