[color]
  ui = false
[alias]
  root=//:root_repo
[java]
  source_level = 7
[project]
  ignore = buck-out
  ide = intellij
[intellij]
  auto_generate_android_facet_sources = false
  jdk_name = Android SDK Version 23
  jdk_type = Android SDK
  language_level = JDK_1_8
  keep_module_files_in_module_dirs = true
