apple_bundle(
    name = "bundle",
    binary = ":noop",
    extension = "bundle",
    info_plist = "Info.plist",
    deps = [
        ":file_resource",
    ],
)

apple_binary(
    name = "noop",
    srcs = ["main.c"],
)

apple_resource(
    name = "file_resource",
    destination = "frameworks",
    dirs = [],
    files = [
        "file.txt",
    ],
    visibility = ["PUBLIC"],
)
