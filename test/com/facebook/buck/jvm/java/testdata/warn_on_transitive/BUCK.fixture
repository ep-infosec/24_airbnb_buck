# This is a canonical example of a genrule() that generates Java source code
# paired with a java_library() that compiles the output of the genrule() along
# with some hard-coded Java source code into a single library.

java_library(
    name = "blargh",
    srcs = ["Blargh.java"],
)

java_library(
    name = "meh",
    srcs = ["Meh.java"],
)

java_library(
    name = "foo",
    srcs = ["Foo.java"],
    exported_deps = [
        ":meh",
    ],
    deps = [
        ":blargh",
    ],
)

java_library(
    name = "bar",
    srcs = ["Bar.java"],
    deps = [
        ":blargh",
        ":foo",
    ],
)

java_library(
    name = "raz",
    srcs = ["Raz.java"],
    deps = [":bar"],
)
